{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["src/scripts/main/public/js"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [],
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "template": "node_modules/jsdoc/templates/default"
  }
}
